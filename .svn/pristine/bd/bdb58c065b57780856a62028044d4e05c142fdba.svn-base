#top{
		width:100%;
		height:30px;
		padding: 5px 0; 	
		
	}
	#top .select{
		height:30px;
		line-height:30px;
		text-align:center;
		border-right:1px solid #aaa;
		width:24.8%;
		font-size:16px;
		float:left;
	}
	#top .last{
		border-right:none;
	}
	#msg{
		clear: both;
		height:30px;
		width:100%;
		line-height: 30px;
		background: #ddd;
	}
	
	
	#msg .info{
		font-size:14px;
		color:#303030;
		margin:0  0 0 8px;;
		float:left;
	}
	#msg .count{
		font-size:14px;
		color:#393939;
		margin:0 15px 0 0;
		float:right;
	
	}
	#main{
		clear:both;
		padding: 5px 0 0 0;
	}
	#main .car_list{
   		width:50%;
   		overflow: hidden;
   		float: left;
   	
   	}
 	#main .car_list img{
 		width:92%;
   		height:140px;
   		margin:0 auto;
   		display:block;
   		border-radius: 5px;
 	} 	
    #main .car_list	figcaption{
    	margin: 5px 0 0 0;
    }
   	#main .car_list figcaption .title{
   		padding:0 5px;
   		height:40px;
   		line-height:20px;
   		font-size:14px;
   		border:none;
   	
   	}
   	#main .car_list figcaption .info{
   		font-size:12px;
   		color:#999;
   		margin:0 0 0 5px;
   		
   	}
   	#main .car_list figcaption .price{
   		color:#FB0606;
   		font-size:16px;
   		padding: 2px 0 5px 8px;
   	}
   	#main .car_list figcaption .hid{
   		display:none;
   	}